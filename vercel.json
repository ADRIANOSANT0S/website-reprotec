{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/index.html" },
    { "src": "/sobre", "dest": "/dist/about.html" },
    { "src": "/servicos", "dest": "/dist/services.html" },
    { "src": "/contato", "dest": "/dist/contact.html" },
    { "handle": "filesystem" }
  ]
}
