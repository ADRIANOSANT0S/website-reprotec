{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/dist/index.html" },
    { "source": "/sobre", "destination": "/dist/about.html" },
    { "source": "/servicos", "destination": "/dist/services.html" },
    { "source": "/contato", "destination": "/dist/contact.html" }
  ]
}
